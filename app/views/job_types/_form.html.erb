    <%= form_for(job_type) do |form| %>
      <% if job_type.errors.any? %>
        <ul>
          <% job_type.errors.full_messages.each do |error| %>
            <li><%= error %></li>
          <% end %>
        </ul>
      <% end %>

      <div class="form">
        <%= form.label :"Job Type Title" %>
        <%= form.text_field :title, class: '' %>
      </div>
      <br>
      <div class="form">
        <%= form.submit "Submit", class: 'button' %>
      </div>
    <% end %>
