<tr>
  <td><%= link_to all_resume.first_name + " " + all_resume.last_name, all_resume, remote: true %></td>
  <td><%= all_resume.work_experiences[0].job_title if all_resume.work_experiences.count > 0 %></td>
  <td><%= all_resume.degree %></td>
  <td><%= all_resume.desired_job_title %></td>
  <td><%= all_resume.created_at.strftime("%B %d, %Y") %></td>
  <td><%= link_to '', all_resume, class: 'fa fa-eye fa-lg' %></td>
	<% if user_signed_in? %>
    <td>
      <% if current_user.bookmark_resume?(all_resume) %>
        <%= link_to '', unbookmark_resume_path(resume_id: all_resume.id), class: 'fa fa-star fa-2x unbookmark', title: 'Unbookmark!', method: :delete %>
      <% else %>
        <%= link_to '', bookmark_resume_path(resume_id: all_resume.id), class: 'fa fa-star fa-2x bookmark', alt: 'Bookmark!', title: 'Bookmark!' %>
      <% end %>
    </td>
	<% end %>
</tr>
